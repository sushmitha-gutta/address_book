<table border="1">
	<tr>
		<th>name</th>
		<th>email</th>
		<th>phone</th>
		<th>birthday</th>
		<th>active</th>
		<th>address</th>
		<th>city</th>

		<th>state</th>
		<th>zipcode</th>
		<th>image</th>

		<% @contacts.each do |contact| %>
		<tr>
			<td><%= contact.name %></td>
			<td><%= contact.email %></td>
			<td><%= contact.phone %></td>
			<td><%= contact.birthday %></td>
			<td><%= contact.active %></td>
			<td><%= contact.address %></td>
			<td><%= contact.city %></td>
			<td><%= contact.state %></td>
			<td><%= contact.zipcode %></td>
			<td><%= contact.image %></td>
		</tr>
<% end %>
</table>
